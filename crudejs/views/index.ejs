<html lang="en">
<head>
    <title>Document</title>
    <link rel="stylesheet" href="	https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
</head>
<body>
    <h2>All Data Get</h2>
    <table border="2px" class="table table-info table-striped">
        <tr>
        <th>id</th>
        <th>name</th>
        <th>age</th>
        <th>email</th>
        <th>city</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    <% data.forEach((item)=>{%>
<tr>
    <td><%= item._id%></td>
    <td><%= item.name%></td>
    <td><%= item.age%></td>
    <td><%= item.email%></td>
    <td><%= item.city%></td>
    <td><a href="edit/<%= item._id%>">Edit</a></td>
<td><a href="delete/<%= item._id %>">delete</a></td>
</tr>



        <%})%>
    </table>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>